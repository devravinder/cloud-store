{
  "functions": [
    {
      "source": "dist",
      "codebase": "default",
      "disallowLegacyRuntimeConfig": true,
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm run build"
      ]
    }
  ],
  "emulators": {
    "port": 9000,
    "functions": {
      "port": 3000
    }
  }
}
