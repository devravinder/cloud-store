{
  "functions": [
    {
      "codebase": "default",
      "disallowLegacyRuntimeConfig": true,
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": "npm run firebase:build",
      "source": "firebase-src"
    }

  ],
  "emulators": {
    "port": 9000,
    "functions": {
      "port": 3000
    }
  }
}
